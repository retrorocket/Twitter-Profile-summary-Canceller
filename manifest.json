{
	"name": "@Twitter Profile summary Canceller",
		"description": "Twitterのモーダルウィンドウをキャンセルして直接ユーザーページに飛べるようにする拡張機能",
		"version": "1.1.2",
		"permissions": [
			"https://twitter.com/*", "http://twitter.com/*"
			],
		"content_scripts": [
		{
			"matches":  ["https://twitter.com/*", "http://twitter.com/*"],
			"js": ["jquery.js","script.js"],
			"run_at": "document_idle"
		}
	],
		"icons": {
			"128": "./images/large.png",
			"48": "./images/mid.png"
		},
		"manifest_version":2
}

